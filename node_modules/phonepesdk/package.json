{
  "name": "phonepesdk",
  "version": "0.3.18",
  "description": "SDK for integration into PhonePe",
  "main": "./artifacts/core/PhonePe",
  "types": "./artifacts/index.d.ts",
  "scripts": {
    "tsc": "tsc",
    "flow": "flow; test $? -eq 0 -o $? -eq 2",
    "webpack": "webpack",
    "clean": "rm -rf lib",
    "start": "webpack-dev-server --hot --inline",
    "webpackProd": "export NODE_ENV=production && npx webpack -p",
    "moduleExportProd": "export NODE_ENV=production && webpack --config websdk.config.js",
    "customPublish": "rm -rf artifacts && tsc && npm publish",
    "publishBeta": "rm -rf artifacts && tsc && npm publish --tag beta"
  },
  "repository": {
    "type": "git",
    "url": "git@gitlab.phonepe.com:react/phonepe-js-sdk.git"
  },
  "author": "PhonePe Pvt. Limited",
  "license": "ISC",
  "devDependencies": {
    "@types/react-native": "^0.52.25",
    "awesome-typescript-loader": "^4.0.1",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.1",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "tslint": "^5.8.0",
    "typescript": "2.7.2",
    "uglifyjs-webpack-plugin": "^1.0.0-rc.0",
    "webpack": "^4.20.2",
    "webpack-cli": "^3.1.1"
  },
  "peerDependencies": {
    "react": "*",
    "react-native": "*",
    "js-base64": "2.5.1"
  }
}
